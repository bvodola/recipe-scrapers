[run]
branch = True
source = bvodola_recipe_scrapers

omit = bvodola_recipe_scrapers/_abstract.py
       bvodola_recipe_scrapers/__init__.py
       bvodola_recipe_scrapers/__version__.py

[report]
exclude_lines =
    pragma: no cover

    # Don't complain if tests don't hit defensive assertion code:
    raise AttributeError
    raise NotImplementedError

ignore_errors = True
